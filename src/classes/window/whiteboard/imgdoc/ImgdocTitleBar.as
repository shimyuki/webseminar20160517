package window.whiteboard.imgdoc {	import flash.display.*;	import flash.geom.*;	import window.TitleBar;	import common.*;	import window.whiteboard.OpenCloseBtn;		// サイズ変更可能タイトルバー	public class ImgdocTitleBar extends TitleBar {		public function ImgdocTitleBar( leftCon) {						super( leftCon, null, 100);					}				override public function setViewWidth( w:Number, debug:String = ""):void {			super.setViewWidth( w, debug);						if( w <= OpenCloseBtn.W) m_leftCon_1st.visible = false;			else m_leftCon_1st.visible = true;		}			}}