package window.whiteboard.slide {	import flash.display.*;	import flash.geom.*;	import common.*;	import window.*;	import flash.text.*;	import flash.events.*;	import flash.net.*;		public class PaintPartsData {		public var id:String = "";		public var tool_name:String = "";		public var depth:uint = 0;		public var visible:Boolean = true;				public var lineweight:Number = 0;		public var linecolor:Number = 0;		public var linealpha:Number = 1;		public var fillcolor:Number = 0;		public var fillalpha:Number = 1;		public var posi_x:Number = 0;		public var posi_y:Number = 0;		public var size_w:Number = 0;		public var size_h:Number = 0;		public var lineTo_x:Array = null;		public var lineTo_y:Array = null;		public var fontsize:Number = 12;		public var fontname:String = "";		public var fontbold:Boolean = false;		public var text:String = "";		public var scaleX:Number = 1;		public var scaleY:Number = 1;				public var lastUpdateTime:Number = 0; // 最終更新日				public function PaintPartsData( tool_name:String, depth:uint, _id:String, _lastUpdateTime:Number) {			this.tool_name = tool_name;			this.depth = depth;									if( _id != "") {				// 共有オブジェクト読み込みにより生成されたとき				id = _id;				lastUpdateTime = _lastUpdateTime;			} else {				// 自分自身のペイントツールにより生成されたとき				// id 生成				// 時刻+乱数にする				var now:Date = new Date();				id = String( now.getTime()) + String( Math.floor( Math.random() * 1000));								lastUpdateTime = now.getTime();								this.visible = true;			}						lineTo_x = new Array();			lineTo_y = new Array();		}					}}