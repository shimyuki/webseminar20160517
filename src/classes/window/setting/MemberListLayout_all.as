package window.setting {	import flash.display.*;	import fl.controls.ComboBox;	import fl.events.*;	import common.*;	import window.*;	import partition.*;	import flash.events.Event;		// 環境設定の画面モード作成／編集（ビュー）用コンポーネントセットの、受講生一覧の分割	public class MemberListLayout_all extends MemberListLayout {		public function MemberListLayout_all() {			super();		}				// uidArrがnullの場合は全員表示(講師も含めて)		public function init( cols:int, uidArr:Array = null) {			super.reset( cols, uidArr); // m_uidArrとm_cellArrをリセット						// セル配列生成（とりあえず全員表示して、全員「使用しない」に設定）			var posi_x = 0;			var posi_y = 0;			var cell:Cell_all;			var uid:String;						for each( uid in m_allUidArr) {				cell = Cell_all( addChild( new Cell_all( uid, ( W - PAD * ( cols - 1)) / cols)));							m_cellArr.push( cell);				cell.x = posi_x;				cell.y = posi_y;				posi_x += cell.getViewWidth() + PAD;				if( posi_x + cell.getViewWidth() > W) {					// 次は新しい行					posi_x = 0;					posi_y += cell.getViewHeight() + PAD;				}				cell.setUse( false);			}						// m_uidArrに入っている人を「使用する」に設定			for each( uid in m_uidArr) {				for each( cell in m_cellArr) {					if( cell.getUid() == uid) cell.setUse( true);				}			}						heightChanged();		}	}}