package window.setting {	import fl.events.*;	import flash.display.*;	import flash.text.*;	import flash.events.*;		class OneCombo_preset extends OneCombo {		private var m_text:TextField;		public function OneCombo_preset() {			graphics.beginFill( 0x446787);			graphics.drawRect( 0, 0, W, H);			graphics.endFill();			graphics.beginFill( 0xf0f0f0);			graphics.drawRect( 8, 8, W-16, H-16);			graphics.endFill();			m_text = TextField( addChild( new TextField()));			m_text.defaultTextFormat = new TextFormat( Main.CONF.getMainFont(), 11, 0x555555);			m_text.text = "";			m_text.x = m_text.y = 9;			m_text.width = W-16;		}				override public function initItems( winNameArr:Array) {			// do nothing		}		override public function selectItem( winName:String) {			m_text.text = winName;		}		override public function getSelectedData() : String {			if( m_text.text == "") return null;			return m_text.text;		}		override public function setEnabled( b:Boolean) {			// do nothing		}	}}