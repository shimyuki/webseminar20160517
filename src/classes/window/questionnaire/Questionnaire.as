package window.questionnaire {			// アンケート情報格納クラス	public class Questionnaire {				public var createDate:String = "";		public var title:String = "";		public var description:String = "";		public var selection:Array = null;		public var uidAnswerHash:Object = null; // uidと回答の連想配列		public var showall:uint = 0;				public function Questionnaire( title:String,									  description:String,									  _selection:Array,									  uidAnswerHash:Object,									  showall:uint,									  createDate:String) {			this.title = title;			this.description = description;			//this.selection = _selection;			if( _selection) {				this.selection = new Array();				for each( var str:String in _selection) {					this.selection.push( str);				}			}			this.uidAnswerHash = uidAnswerHash;			this.showall = showall;			this.createDate = createDate;//var tmp = "";//for each( var t in selection) { tmp += String( t) + ",";}//Main.addDebugMsg( "Questionnaire: " + tmp);					}				public function updateByHash( hash:Object) {			if( hash.title != undefined) title = hash.title;			if( hash.description != undefined) description = hash.description;			if( hash.selection != undefined) {				if( selection != null) {					while( selection.length) selection.pop();				} else {					selection = new Array();				}				for each( var str:String in hash.selection) {					selection.push( str);				}			}			if( hash.uidAnswerHash != undefined) {				uidAnswerHash = new Object();				for (var key:String in hash.uidAnswerHash) {					uidAnswerHash[ key] = hash.uidAnswerHash[ key];				}			}			if( hash.showall != undefined) showall = hash.showall;		}				public function getDataHash() : Object {			// この項目を増やしたら、LoadConfのupdateAnswerArr()にも追加すべし。			var hash:Object = new Object();			hash.title = title;			hash.description = description;			hash.selection = selection;			hash.uidAnswerHash = uidAnswerHash;			hash.showall = showall;			hash.createDate = createDate;			return hash;		}	}}