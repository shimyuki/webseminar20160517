package window.video {	import flash.net.*;	import flash.events.SyncEvent;	import flash.display.Shape;	import common.Member;		// 受講生全員の声	public class MemberVoice {				private var m_nsArr:Array;				public function MemberVoice() {			// 人数分のネットストリーム格納配列			m_nsArr = new Array();		}				public function initSo( nc:NetConnection) : void {			var arr:Array = Main.CONF.getMemberArr();			for each( var member:Member in arr) {								// 講師は除く				if( Main.CONF.isPro( member.uid)) continue;								var ns:MyNetStream = new MyNetStream( nc, member.uid);				ns.receiveVideo( false);				ns.play( member.uid);				m_nsArr.push( ns);			}		}		public function getNetStream( uid:String) : MyNetStream {			for each( var ns:MyNetStream in m_nsArr) {				if( ns.getId() == uid) return ns;			}			return null;		}	}}